    },
    {
      "startTime": 1076.056,
      "endTime": 1079.362,
      "text": "Görünüşe göre İsviçre ve Avustralya'dan gelmiş ve size söylüyorum."
    },
    {
      "startTime": 1081.998,
      "endTime": 1083.136,
      "text": "Sabahi: Devam et, Edip."
    },
    {
      "startTime": 1084.086,
      "endTime": 1086.326,
