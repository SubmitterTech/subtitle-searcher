    },
    {
      "startTime": 871.819,
      "endTime": 872.52,
      "text": "\"Evet\""
    },
    {
      "startTime": 872.925,
      "endTime": 875.592,
      "text": "\"Bu ayetin iki anlamı var, iki anlamlı.\""
    },
    {
      "startTime": 875.896,
      "endTime": 880,
